<body class="countainer">
    <form action="/add" method="post">
        <input type="text" name="nama" placeholder="insert your name">
        <input type="number" name="height" placeholder="insert your height">
        <input type="number" name="weight" step="0.0001" placeholder="insert your weight">
        <input type="date" name="birthdate">
        <select name="married">
            <option disabled selected>Have you married ?</option>
            <option value="true">true </option>
            <option value="false">false </option>
        </select>
        <button type="submit" class="save">save</button>
        <a href="/" class="cancel">cancel</a>
    </form>
</body>

</html>